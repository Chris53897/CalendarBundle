{
    "name": "tattali/calendar-bundle",
    "description": "Provides event calendar for your Symfony 4 project. Compatible with Doctrine ORM & ODM, and custom storages.",
    "type": "symfony-bundle",
    "keywords": ["fullcalendar", "calendar", "jquery calendar", "symfony calendar"],
    "homepage": "https://github.com/tattali/CalendarBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ThÃ©o Attali",
            "email": "theoattali@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.4 || ^4.0"
    },
    "require-dev": {
        "dragonrun1/phpspec-code-coverage": "^4.2"
    },
    "suggest": {
        "symfony/orm-pack": "To support Doctrine ORM and Migration.",
        "doctrine/mongodb-odm-bundle": "To support MongoDB."
    },
    "autoload": {
        "psr-4": {
            "CalendarBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CalendarBundle\\": "tests/"
        }
    }
}
